def call(Map params){
	pipeline{
		environment {
            docker_image = ''
        }
        
        agent any
        
        stages {
            stage('Github stage') {
                steps {
                  
                }
            }
            
            stage('Build Stage') {
                steps {
                    script {
                     echo ${params.main}
                    }
                }
            }
            
            stage('Push Stage') {
                steps {
                    script{
                        
                        }
                    }
                }
            }
        }
		
		
		post{
			success {
				script {
        	sh "docker image prune -a -f"
				}
			}
		}
	}
}
